        type: 'CREATE_TABLE'
        c0:
            type: 'TABLE_NAME'
            c0:
                type: 'DOT'
                c0:
                    type: 'VALUE'
                    value: 'table_name'
                c1:
                    type: 'VALUE'
                    value: 'info'
        c1:
            type: 'TABLE_COLUMNS'
            c0:
                type: 'COLUMN_DEFINE'
                value: 'a'
                c0:
                    type: 'CORE_TYPE'
                    value: 'string'
            c1:
                type: 'COLUMN_DEFINE'
                value: 'b'
                c0:
                    type: 'CORE_TYPE'
                    value: 'integer'
    
    dsl:
        index: 'table_name'
        doc_type: 'info'
        body:
            info:
                properties:
                    a:
                        type: 'string'
                    b:
                        type: 'integer'

